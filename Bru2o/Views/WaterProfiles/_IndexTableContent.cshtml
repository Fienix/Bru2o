@model IEnumerable<Bru2o.Models.WaterProfile>

@foreach (var item in Model)
{
    <tr>
        <td><a href="/WaterProfiles/Details/@item.ID">@Html.DisplayFor(modelItem => item.Title)</a></td>
        <td>@Html.DisplayFor(modelItem => item.CreateDate)</td>
        <td>@Html.DisplayFor(modelItem => item.ModifyDate)</td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
            <a href="#deleteModal" data-toggle="modal" data-name="@item.Title" data-id="@item.ID">Delete</a>
        </td>
    </tr>
}